{
  "name": "email-preference-graphql-wrapperq",
  "version": "1.0.0",
  "description": "A GraphQL wrapper for the Mailchimp 3.0 REST API",
  "main": "src/server.js",
  "scripts": {
    "build": "webpack --config scripts/webpack.$NODE_ENV.js",
    "build:dev": "NODE_ENV=development yarn build",
    "dev": "concurrently \"yarn build:dev\" \"nodemon --require dotenv/config dist/server\"",
    "start": "node ./dist/server",
    "test": "jest",
    "test:coverage": "jest --collectCoverage"
  },
  "author": "Michael Foley <m@michaeldfoley.com> (http://michaeldfoley.com)",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.5.0",
    "apollo-server": "^2.8.1",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "lodash": "^4.17.14",
    "md5": "^2.2.1",
    "node-cache": "^4.2.0",
    "p-queue": "^6.0.2",
    "webpack-dev-server": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@types/jest": "^24.0.15",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.15",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/__tests__/__utils.js"
    ]
  }
}